  <MasterDetailPage.Detail>
    <NavigationPage>
      <x:Arguments>
                <pages:BSP_MasterPageDetail />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>